<h2>Go on a date!<h2>

  <form action="/paldates" method="post">

    <label for="palship_id">Client:</label>
    <select name="palship_id" id="palship_id"
    <% @palships.each do |palship| %>
    <option value="<%= palship.id %>"><%= palship.client.name %></option>
    <% end %>
  </select>
  
    <label for="palship_id">Pal:</label>
    <select name="palship_id" id="palship_id"
    <% @palships.each do |palship| %>
    <option value="<%= palship.id %>"><%= palship.pal.name %></option>
    <% end %>
  </select>
  <p><p>

    <label for="event_id">Event:</label>
    <select name="event_id" id="event_id">
      <% @events.each do |event| %>
      <option value="<%= event.id %>"><%= event.type %></option>
      <% end %>
    </select>

    <input type="submit" value="Take your pal on a date!">
  
  </form>
